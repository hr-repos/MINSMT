<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <AssemblyName>SmartChessDashboard</AssemblyName>
    <RootNamespace>SmartChessDashboard</RootNamespace>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="ChessDotNet" Version="1.0.0" />
    <PackageReference Include="MQTTnet" Version="5.0.1.1416" />
  </ItemGroup>

  <ItemGroup>
    <None Include="StockfishEngine\stockfish-windows-x86-64-avx2.exe" CopyToOutputDirectory="Always" />
  </ItemGroup>

</Project>
